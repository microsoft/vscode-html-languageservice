{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 6004,
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/lib/umd/**"]
		},
		{
			"name": "Unit Tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": [
				"--timeout",
				"999999",
				"--colors"
			],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [],
			"env": {},
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/lib/umd/**"],
			"preLaunchTask": "npm: compile"
		}
	]
}